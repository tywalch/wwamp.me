{
  "watch": [
    "."
  ],
  "ignore": [
    "dist/**/*",
    "node_modules/**/*",
    "public/scripts/wwamp.js",
    "public/scripts/wwamp.js.map"
  ],
  "ext": "ts,js,json,hbs",
  "exec": "npm run build:api && node dist/bin/www",
